<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="user_style.css">
</head>
<body>
    <div class="header">
        <h1>DateMate</h1>
        <ul class="first_h">
            <li>
                <a href="">Total Dates</a>
            </li>
            <li class="profile">
                <img class="header_img" src="data:image/jpeg;base64," alt="tangiro">
                <div>
                    <ul>
                        <li>
                            <a href="/profile">Profile</a>
                        </li>
                        <li>
                            <a href="/profile">Find Match</a>
                        </li>
                    </ul>
                </div>
            </li>

        </ul>
    </div>

    <script>
        const drop_down = document.querySelector('.profile div');
        document.querySelector('.header_img').addEventListener('mouseover',function() {
            drop_down.style.display='block';
        });

        window.onclick = function(event) {
            if (!event.target.matches('.header_img')) {
                drop_down.style.display='none';
            }
        }
    </script>
</body>
</html>